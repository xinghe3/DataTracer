<Window x:Class="CN.MACH.Aop.DataTracer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CN.MACH.Aop.DataTracer"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid >
            <Grid.ColumnDefinitions>
                <ColumnDefinition />
                <ColumnDefinition Width="45" />
            </Grid.ColumnDefinitions>
            <TextBox x:Name="searchKeyWords">
                
            </TextBox>
            <Button Grid.Column="1" Click="Button_Click" Content="s">
                
            </Button>
        </Grid>
        <DataGrid Grid.Row="1" ItemsSource="{Binding Records}">
            
        </DataGrid>
        <!--<DataGrid Grid.Row="2" ItemsSource="{Binding RecordDetails}">

        </DataGrid>-->
    </Grid>
</Window>
